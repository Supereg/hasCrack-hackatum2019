{
  "name": "hackatum2019-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && tsc && npm run copy",
    "copy": "cp -r src/bin dist/ && cp -r src/public dist/ && cp -r src/views dist/",
    "start": "cd dist && node bin/www && cd ..",
    "debug": "cd dist && DEBUG=hackatum2019-ui:* node bin/www && cd ..",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "^2.0.4",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.12",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/debug": "^4.1.5",
    "typescript": "^3.7.2",
    "ts-node": "^8.5.2",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0"
  }
}
